{
  "pages": [
    {
      "name": "Index",
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "name": "My",
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "name": "Test",
      "path": "pages/test-page/test-page",
      "style": {
        "navigationBarTitleText": "测试"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "name": "Theme",
      "path": "pages/theme/theme",
      "style": {
        "navigationBarTitleText": "主题"
      },
      "meta": {
        "ignoreAuth": true
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "name": "List",
          "path": "list/list",
          "style": {
            "navigationBarTitleText": "演示列表",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "white"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages-sub/list/list": {
      "network": "all",
      "packages": ["pages-sub"]
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "custom": true,
    "color": "#999",
    "selectedColor": "#000",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)":
        "z-paging/components/z-paging$1/z-paging$1.vue",
      "^z-tabs": "@zxlee/z-tabs/components/z-tabs/z-tabs.vue",
      "^zq-(.*)": "@/components/zq-ui/zq-$1/zq-$1.vue"
    }
  }
}
